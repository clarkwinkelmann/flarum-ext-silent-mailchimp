{
    "name": "clarkwinkelmann/flarum-ext-silent-mailchimp",
    "description": "Silently add new users to a Mailchimp list",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": ">=0.1.0-beta.11 <0.1.0-beta.14",
        "guzzlehttp/guzzle": "^6.0"
    },
    "authors": [
        {
            "name": "Clark Winkelmann",
            "email": "clark.winkelmann@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "ClarkWinkelmann\\SilentMailchimp\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Silent Mailchimp",
            "icon": {
                "name": "fab fa-mailchimp",
                "backgroundColor": "#684ba6",
                "color": "#fff"
            }
        },
        "flagrow": {
            "discuss": "https://discuss.flarum.org/d/23079"
        }
    }
}
